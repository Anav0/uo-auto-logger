{
  "manifest_version": 2,
  "name": "UO auto logger",
  "version": "1.2",
  "description": "Automatycznie loguje u≈ºytkownika do sieci UO",
  "icons": {
    "24": "icons/LogoUOlight.svg",
    "48": "icons/LogoUOlight.svg",
    "96": "icons/LogoUOlight.svg"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "browser_style": true,
    "default_icon": "icons/LogoUOlight.svg",
    "default_title": "UO logger",
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://uonet.uni.opole.pl/login*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}